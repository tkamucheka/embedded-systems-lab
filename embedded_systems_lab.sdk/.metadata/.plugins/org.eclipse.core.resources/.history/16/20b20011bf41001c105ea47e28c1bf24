#include "artyBotLib.h"

int main()
{
	artyBotInit();
	// xil_printf("Initialization complete...\r");

	// Example 1: basic driving
	for (int i = 0; i < 4; ++i)
	{
		driveForward(20);
		turnRight(90);
	}
	// xil_printf("Examples finished.\r");



	artyBotEnd();

	return 0;
}



